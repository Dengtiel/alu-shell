#!/usr/bin/env bash
# 8-beheaded_process

# Get the PID of the 7-highlander process
pid=$(pgrep -f "7-highlander")

# Check if the process is running
if [ -n "$pid" ]; then
    # Kill the process
    echo "Killing process $pid..."
    kill "$pid" 2>/dev/null
else
    echo "No running process found for 7-highlander."
fi
