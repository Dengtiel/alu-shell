#!/usr/bin/env bash
# This script lists files in the current directory
# and displays only the part of the name after the first dash.

# Loop through the files in the current directory
for file in *; do
    # Check if the file is not a hidden file
    if [[ $file == -* ]]; then
        # Extract and print the part of the filename after the first dash
        echo "${file#*-}"
    fi
done
